// It's very strange that I can pass the test in the leetcode 
// But exceed time limit in the linkcode
// Please help me 
class Solution {
public:
    /**
     * @param s: A string s
     * @param dict: A dictionary of words dict
     */
    bool wordSegmentation(string s, unordered_set<string> &dict) {
        // write your code here
        int n = s.length();
        vector<bool> store (n + 1,false);
        store[0] = true;
        for (int i = 1; i <= n; i++) {
            for (int k = 0; k < i; k++) {
                store[i] = store[k] && dict.find(s.substr(k, i - k)) != dict.end();
                if (store[i]) break;
            }
        }
        return store[n];
    }
};
